{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app-server",
    "paths": {
      "ng-carousel": [
        "dist/ng-carousel"
      ],
      "ng-carousel/*": [
        "dist/ng-carousel/*"
      ],
      "ng-carousel-cdk": [
        "dist/ng-carousel"
      ],
      "ng-carousel-cdk/*": [
        "dist/ng-carousel/*"
      ]
    },
    "typeRoots": [
      "node_modules/@types"
    ]
},
  "files": [
    "src/main.server.ts",
    "server.ts"
  ],
  "angularCompilerOptions": {
    "entryModule": "./src/app/app.server.module#AppServerModule"
  }
}
